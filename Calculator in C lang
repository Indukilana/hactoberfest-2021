#include <stdio.h>

int main()
{

  int a,b,c,add,sub,mul,Rem;
  float div;
  printf("Enter number 01: ");
  scanf("%d", &a);
  
  printf("Enter number 02: ");
  scanf("%d", &b);

  
  add = a + b;
  sub = a - b;
  mul = a * b;
  div = a / b;
  Rem= a%b
  
  printf("Addition of the two entered numbers: %d", add);
  printf("Subtraction of the two entered numbers: %d", sub);
  printf("Multiplication of the two entered numbers: %d', mul);
  printf("Division of the two entered numbers: %d', div);
  printf("Remainder of the two entered numbers: %d', Rem);
}
